context log {

entity errors {
    ERROR_TIMESTAMP: UTCDateTime;
    PARAMETER: String(256);
    SQL_ERROR_CODE: Integer;
    SQL_ERROR_MESSAGE: String(5000);
 };

entity messages {
    ERROR_TIMESTAMP: UTCDateTime;
    PARAMETER: String(256);
    SQL_ERROR_CODE: Integer;
    SQL_ERROR_MESSAGE: String(5000);
 };

};
