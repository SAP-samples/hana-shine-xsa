                                            <sap.ui.commons.layout:rows>
                                                <sap.ui.commons.layout:MatrixLayoutRow>
                                                    <sap.ui.commons.layout:cells>
                                                        <sap.ui.commons.layout:MatrixLayoutCell>
                                                            <sap.ui.commons.layout:content>
                                                                <sap.viz.ui5:Column id="salesCompareColumn" width="100%" height="380px">
                                                                    <sap.viz.ui5:toolTip>
                                                                        <sap.viz.ui5.types:Tooltip formatString="#,###,###,###.00">
                                                                        </sap.viz.ui5.types:Tooltip>
                                                                    </sap.viz.ui5:toolTip>
                                                                </sap.viz.ui5:Column>
                                                            </sap.ui.commons.layout:content>
                                                        </sap.ui.commons.layout:MatrixLayoutCell>
                                                    </sap.ui.commons.layout:cells>
                                                </sap.ui.commons.layout:MatrixLayoutRow>
                                            </sap.ui.commons.layout:rows>
                                        </sap.ui.commons.layout:MatrixLayout>
                                    </sap.ui.commons:content>
                                    <sap.ui.commons:buttons>
                                        <sap.ui.commons:Button class="helpButton" text="?" data:tileDialog="5" press="openTileDialog"></sap.ui.commons:Button>
                                    </sap.ui.commons:buttons>
                                </sap.ui.commons:Panel>
                            </sap.ui.commons.layout:content>
                        </sap.ui.commons.layout:MatrixLayoutCell>
                    </sap.ui.commons.layout:cells>
                </sap.ui.commons.layout:MatrixLayoutRow>
                <sap.ui.commons.layout:MatrixLayoutRow>
                    <sap.ui.commons.layout:cells>
                        <sap.ui.commons.layout:MatrixLayoutCell>
                            <sap.ui.commons.layout:content>
                                <sap.ui.commons:Panel text="{i18n>PRODUCT_SALES}">
                                    <sap.ui.commons:content>
                                         <sap.ui.commons.layout:MatrixLayout>
                                            <sap.ui.commons.layout:rows>
                                                <sap.ui.commons.layout:MatrixLayoutRow>
                                                    <sap.ui.commons.layout:cells>
                                                        <sap.ui.commons.layout:MatrixLayoutCell>
                                                            <sap.ui.commons.layout:content>
                                                                <sap.viz.ui5:Scatter id="productSalesScatter" width="100%" height="420px">
                                                                    
