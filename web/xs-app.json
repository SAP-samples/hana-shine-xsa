{
  "welcomeFile": "launchpad/index.html",
  "logout": {
    "logoutEndpoint": "/logout",
    "logoutPage": "/"
  },
  "routes": [
    {
      "source": "/sap/rest/(.*)",
      "destination": "uaa"
    },
    {
      "source": "/sap/hana/democontent/epm/services/",
      "destination": "core-backend"
    },
    
    {
      "source": "/sap/hana/democontent/epm/spatial/services/",
      "destination": "core-backend"
    },
    {
      "source": "/get/(.*)",
      "destination": "core-backend",
      "scope": "$XSAPPNAME.DataGenerator"
    },
    {
      "source": "/reset/(.*)",
      "destination": "core-backend",
      "scope": "$XSAPPNAME.DataGenerator"
    },
    {
      "source": "/replicate/(.*)",
      "destination": "core-backend",
      "scope": "$XSAPPNAME.DataGenerator"
    },
    {
      "source": "/jobs/(.*)",
      "destination": "user-backend"
    },
    {
      "source": "/schedules/(.*)",
      "destination": "user-backend"
    },
    {
      "source": "/jobactivity/(.*)",
      "destination": "user-backend",
      "authenticationType": "none"
    },
    {
      "source": "/user/xsodata/(.*)",
      "destination": "user-backend",
      "authenticationType": "none"
    },
    {
      "source": "^/admin-ui/(.*)",
      "localDir": "resources",
      "scope": "$XSAPPNAME.DataGenerator",
      "replace": {
        "pathSuffixes": [
          "index.html"
        ],
        "vars": [
          "sapui5url"
        ]
      }
    },
    {
      "source": "^/user/odata/v4/(.*)",
      "destination": "user-java-backend"
    },
    {
      "source": "^/(.*)",
      "localDir": "resources",
      "cacheControl": "public, max-age=1000,must-revalidate",
      "replace": {
        "pathSuffixes": [
          "index.html"
        ],
        "vars": [
          "sapui5url"
        ]
      }
    }
  ]
}
